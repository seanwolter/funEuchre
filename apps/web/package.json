{
  "name": "@fun-euchre/web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm run build && node dev-server.mjs",
    "start": "node dev-server.mjs",
    "build": "tsc -p ../../packages/protocol/tsconfig.json && tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "tsc -p tsconfig.json --noEmit && eslint --max-warnings=0 dev-server.mjs",
    "test": "tsc -p tsconfig.json && tsc -p tsconfig.test.json && node --test dist-test/test/*.test.js"
  }
}
